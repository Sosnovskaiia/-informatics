/*Напишите программу, вычисляющую сумму нечетных цифр целого числа.*/
 
#include <iostream>
#include <cstring>
 
using namespace std;
int charToInt(char c){
   return c - '0';
}
int main()
{
   string n;
   cin>>n;
   int sum=0;
   for(int i=0;i<n.length();i++)
   {
       
       int m;
       m=charToInt(n.at(i));
       if(m%2==1)
           sum=sum+m;
           
       
   }
   
cout<<sum; 
   return 0;
}
